{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "SnapParse",
  "version": "0.1.28",
  "identifier": "com.leisure462.snapparse",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "SnapParse",
        "width": 1200,
        "height": 780,
        "resizable": true,
        "visible": false
      }
    ]
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": ["../icon_transparent.ico"]
  }
}
